{
  "name": "tsondb",
  "version": "0.5.4",
  "description": "",
  "license": "ISC",
  "author": "Lukas Obermann",
  "type": "module",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "public"
  ],
  "bin": {
    "tsondb": "lib/bin/tsondb.js"
  },
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./renderer/jsonschema": "./lib/node/renderers/jsonschema/index.js",
    "./renderer/ts": "./lib/node/renderers/ts/index.js",
    "./schema": "./lib/node/Schema.js",
    "./schema/def": "./lib/node/schema/index.js"
  },
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "test": "node --import tsx --test",
    "lint": "eslint",
    "check-format": "prettier \"{src,test}/**/*.{ts,tsx}\" --check",
    "format": "prettier \"{src,test}/**/*.{ts,tsx}\" --write",
    "release": "commit-and-tag-version",
    "release:sign": "commit-and-tag-version --sign --signoff"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.13",
    "commit-and-tag-version": "^12.5.1",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {
    "debug": "^4.4.1",
    "express": "^5.1.0",
    "preact": "^10.26.9",
    "preact-iso": "^2.9.2",
    "simple-cli-args": "^0.1.0",
    "simple-git": "^3.28.0",
    "supports-color": "^10.0.0",
    "uuid": "^11.1.0"
  },
  "repository": "github:elyukai/tsondb",
  "bugs": {
    "url": "https://github.com/elyukai/tsondb/issues"
  },
  "engines": {
    "node": ">=23.6.0"
  }
}
