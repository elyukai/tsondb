    "@elyukai/utils": "^0.2.0",
    "@preact/signals": "^2.7.0",
    "debug": "^4.4.3",
    "express": "^5.2.1",
    "messageformat": "^4.0.0",
    "preact": "^10.28.3",
    "preact-iso": "^2.11.1",
    "simple-cli-args": "^0.1.3",
    "simple-git": "^3.30.0"
