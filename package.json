    "./schema/dsl": "./dist/src/node/schema/dsl/index.js",
    "./schema/gen": "./dist/src/node/schema/generatedTypeHelpers.js"
  },
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "test": "node --import tsx --test",
    "lint": "eslint",
    "format": "prettier \"{src,test}/**/*.{ts,tsx}\" --write",
    "format:check": "prettier \"{src,test}/**/*.{ts,tsx}\" --check",
    "release": "commit-and-tag-version",
    "release:sign": "commit-and-tag-version --sign --signoff"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/debug": "^4.1.12",
