{
  "name": "tsondb",
  "version": "0.12.7",
  "description": "",
  "license": "ISC",
  "author": "Lukas Obermann",
  "type": "module",
  "files": [
    "dist/src/**/*.{d.ts,js}",
    "public"
  ],
  "bin": {
    "tsondb": "dist/src/bin/tsondb.js"
  },
  "main": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js",
    "./config": "./dist/src/node/config.js",
    "./renderer/jsonschema": "./dist/src/node/renderers/jsonschema/index.js",
    "./renderer/ts": "./dist/src/node/renderers/ts/index.js",
    "./schema": "./dist/src/node/schema/Schema.js",
    "./schema/def": "./dist/src/node/schema/index.js"
  },
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "test": "node --import tsx --test",
    "lint": "eslint",
    "format": "prettier \"{src,test}/**/*.{ts,tsx}\" --write",
    "format:check": "prettier \"{src,test}/**/*.{ts,tsx}\" --check",
    "release": "commit-and-tag-version",
    "release:sign": "commit-and-tag-version --sign --signoff"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "commit-and-tag-version": "^12.6.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "@preact/signals": "^2.5.1",
    "debug": "^4.4.3",
    "express": "^5.1.0",
    "messageformat": "^4.0.0",
    "preact": "^10.27.2",
    "preact-iso": "^2.11.0",
    "simple-cli-args": "^0.1.3",
    "simple-git": "^3.30.0"
  },
  "repository": "github:elyukai/tsondb",
  "bugs": {
    "url": "https://github.com/elyukai/tsondb/issues"
  },
  "engines": {
    "node": ">=23.6.0"
  }
